<Project Sdk="Godot.NET.Sdk/4.4.1">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <EnableDynamicLoading>true</EnableDynamicLoading>
    <RootNamespace>MangaShaderRenderer</RootNamespace>

    <!-- CA1050: Declare types in namespaces -->
    <!-- CA1822: Mark members as static -->
    <!-- IDE0130: Namespace does not match the folder structure -->
    <!-- IDE0350: Use implicitly typed lambda -->
    <!-- IDE1006: Naming rule violation -->
    <!-- CS8618: Non-nullable field must contain a non-null value when exiting constructor -->
    <NoWarn>CA1050;CA1822;IDE0130;IDE0350;IDE1006;CS8618</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GDTask" Version="1.3.0" />
  </ItemGroup>
</Project>